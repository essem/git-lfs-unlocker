{
  "name": "electron-boilerplate",
  "version": "1.0.0",
  "description": "Electron boilerplate",
  "main": "main.js",
  "homepage": ".",
  "scripts": {
    "start": "npm run build && npm run electron",
    "dev": "concurrently \"npm:react\" \"sleep 2 && DEV_SERVER=\"http://localhost:3000\" npm run electron\"",
    "electron": "electron .",
    "react": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "prettier": "prettier --list-different \"./**/*.js\""
  },
  "repository": "https://github.com/essem/electron-boilerplate",
  "devDependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "asar": "^2.0.1",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "electron": "^7.2.4",
    "i18next": "^17.0.3",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^3.0.0",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.0",
    "react-scripts": "3.0.1",
    "typeface-roboto": "0.0.54"
  },
  "browserslist": [
    "last 1 electron version"
  ]
}
